    "vue-router": "^4.1.6",
    "vuex": "^4.1.0"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.1.0",
    "@vitest/coverage-c8": "^0.30.1",
    "@vue/compiler-sfc": "^3.1.0",
    "@vue/test-utils": "^2.3.2",
    "autoprefixer": "^9.1.5",
    "eslint": "^8.39.0",
    "husky": "^1.1.1",
    "jsdom": "^21.1.1",
    "lint-staged": "^7.3.0",
    "postcss-import": "^12.0.0",
    "postcss-loader": "^3.0.0",
    "postcss-url": "^8.0.0",
    "prettier": "^2.8.8",
    "sass": "^1.62.0",
    "typescript": "^5.0.4",
    "vite": "^4.5.2",
    "vitest": "^0.30.1",
    "vue-eslint-parser": "^9.1.1",
    "vue-tsc": "^1.4.4"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
